{
    "name": "yespbs/flysystem-egnyte",
    "description": "Flysystem Adapter for the Egnyte V1 API",
    "type": "package",
    "require": {
        "php": ">=5.6.4",
        "league/flysystem": "^1.0",
        "yespbs/egnyte": "*"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^2.4@dev",
        "phpunit/phpunit": "^6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Priyabrata Sarkar",
            "email": "yespbs@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Yespbs\\FlysystemEgnyte\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yespbs\\FlysystemEgnyte\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
